<h1 class="page-header">添加币种</h1>

<% provide(:title, '添加币种') %>
<div class="show_con">
  <div class="container-fluid">

    <%= simple_form_for @currency, url: currencies_path,html: {class: 'form-horizontal'} do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= f.submit '添加币种', :class => 'btn btn-success form-control' %>
    <% end %>
  </div>
</div>
