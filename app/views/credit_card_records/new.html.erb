<h1 class="page-header">添加刷卡记录</h1>
<% provide(:title, '添加刷卡记录') %>
<div class="show_con">
  <div class="container-fluid">

    <%= simple_form_for @credit_card_record, url: credit_card_records_path,html: {class: 'form-horizontal'} do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= f.submit '添加刷卡记录', :class => 'btn btn-success form-control' %>
    <% end %>
  </div>
</div>
