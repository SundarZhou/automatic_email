<h1 class="page-header">修改银行卡信息</h1>
<% provide(:title, '修改银行卡信息') %>
<div class="show_con">
  <div class="container-fluid">

    <%= simple_form_for @credit_card, url: credit_card_path(@credit_card),html: {class: 'form-horizontal'} do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= f.submit '修改银行卡信息', :class => 'btn btn-success form-control' %>
    <% end %>
  </div>
</div>
